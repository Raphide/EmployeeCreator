{
    "configurations": [
        {
            "type": "java",
            "name": "EmployeeApplication",
            "request": "launch",
            "mainClass": "io.nology.employee.EmployeeApplication",
            "projectName": "employee"
        },
        {
            "name": "Docker Node.js Launch",
            "type": "docker",
            "request": "launch",
            "preLaunchTask": "docker-run: debug",
            "platform": "node",
            "node": {
                "package": "${workspaceFolder}/employeeFrontend/package.json",
                "localRoot": "${workspaceFolder}/employeeFrontend"
            }
        }
    ]
}